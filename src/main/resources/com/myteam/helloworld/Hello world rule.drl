package com.myteam.helloworld;
dialect "mvel"

rule "second world"
when 
    $hw: HelloWorldObject(firstStr=="Hello")
then
    modify($hw){ 
        secondStr="World"
    };
end;

rule "full string"
when 
    $hw: HelloWorldObject()
then
    modify($hw){ 
        fullStr=$hw.firstStr+" "+$hw.secondStr;
    };
end;


